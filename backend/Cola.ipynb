    "import tkinter as tk\n",
    "from tkinter import messagebox\n",
    "from tkinter import ttk\n",
    "\n",
    "# Crear ventana principal\n",
    "ventana = tk.Tk()\n",
    "ventana.title(\"Gestión de Productos\")\n",
    "\n",
    "# Frame para la entrada de datos del producto\n",
    "frame_entrada = tk.Frame(ventana)\n",
    "frame_entrada.pack(pady=10)\n",
    "\n",
    "\n",
    "tk.Label(frame_entrada, text=\"Registro de productos:\").grid(row=0, column=0, padx=5, pady=5)\n",
    "\n",
    "# Etiquetas y entradas de texto\n",
    "tk.Label(frame_entrada, text=\"Nombre del Producto:\").grid(row=1, column=0, padx=5, pady=5)\n",
    "entrada_nombre = tk.Entry(frame_entrada)\n",
    "entrada_nombre.grid(row=1, column=1, padx=5, pady=5)\n",
    "\n",
    "tk.Label(frame_entrada, text=\"Tipo del producto:\").grid(row=2, column=0, padx=5, pady=5)\n",
    "entrada_nombre = tk.Entry(frame_entrada)\n",
    "entrada_nombre.grid(row=2, column=1, padx=5, pady=5)\n",
    "\n",
    "tk.Label(frame_entrada, text=\"Categoría\").grid(row=3, column=0, padx=5, pady=5)\n",
    "entrada_nombre = tk.Entry(frame_entrada)\n",
    "entrada_nombre.grid(row=3, column=1, padx=5, pady=5)\n",
    "\n",
    "tk.Label(frame_entrada, text=\"Cantidad:\").grid(row=4, column=0, padx=5, pady=5)\n",
    "entrada_cantidad = tk.Entry(frame_entrada)\n",
    "entrada_cantidad.grid(row=4, column=1, padx=5, pady=5)\n",
    "\n",
    "tk.Label(frame_entrada, text=\"Precio:\").grid(row=5, column=0, padx=5, pady=5)\n",
    "entrada_cantidad = tk.Entry(frame_entrada)\n",
    "entrada_cantidad.grid(row=5, column=1, padx=5, pady=5)\n",
    "\n",
    "tk.Label(frame_entrada, text=\"Fecha de agregación:\").grid(row=6, column=0, padx=5, pady=5)\n",
    "entrada_precio = tk.Entry(frame_entrada)\n",
    "entrada_precio.grid(row=6, column=1, padx=5, pady=5)\n",
    "\n",
    "tk.Label(frame_entrada, text=\"Fecha de vencimiento:\").grid(row=7, column=0, padx=5, pady=5)\n",
    "entrada_precio = tk.Entry(frame_entrada)\n",
    "entrada_precio.grid(row=7, column=1, padx=5, pady=5)\n",
    "\n",
    "tk.Label(frame_entrada, text=\"Gestión de productos\").grid(row=0, column=4, padx=50, pady=5)\n",
    "tk.Label(frame_entrada, text=\"Buscar:\").grid(row=1, column=4, padx=5, pady=5)\n",
    "entrada_precio = tk.Entry(frame_entrada)\n",
    "entrada_precio.grid(row=1, column=5, padx=5, pady=5)\n",
    "\n",
    "def mostrar_seleccion():\n",
    "    seleccion = combobox_opciones.get()\n",
    "    if seleccion:\n",
    "        messagebox.showinfo(\"Opción Seleccionada\", f\"Has seleccionado: {seleccion}\")\n",
    "    else:\n",
    "        messagebox.showwarning(\"Advertencia\", \"No has seleccionado ninguna opción.\")\n",
    "\n",
    "# Crear un Frame para el Combobox\n",
    "frame_opciones = tk.Frame(ventana)\n",
    "frame_opciones.pack(pady=10)\n",
    "\n",
    "# Etiqueta para el menú desplegable\n",
    "tk.Label(frame_opciones, text=\"Elige una opción:\").grid(row=2, column=6, padx=5, pady=5)\n",
    "\n",
    "# Crear Combobox\n",
    "opciones = [\"Productos perecibles\", \"Productos no perecibles\", \"Del último al primero\"]  # Lista de opciones\n",
    "combobox_opciones = ttk.Combobox(frame_opciones, values=opciones, state=\"readonly\")\n",
    "combobox_opciones.grid(row=2, column=6, pady=5)\n",
    "\n",
    "# Botón para mostrar la opción seleccionada\n",
    "boton_mostrar = tk.Button(ventana, text=\"Mostrar Selección\", command=mostrar_seleccion)\n",
    "boton_mostrar.pack(pady=10)\n",
    "\n",
    "\n",
    "# Funciones para manejar los productos\n",
    "productos = []\n",
    "\n",
    "def agregar_producto():\n",
    "    nombre = entrada_nombre.get()\n",
    "    cantidad = entrada_cantidad.get()\n",
    "    precio = entrada_precio.get()\n",
    "    \n",
    "    if nombre and cantidad and precio:\n",
    "        producto = f\"Nombre: {nombre}, Cantidad: {cantidad}, Precio: {precio}\"\n",
    "        productos.append(producto)\n",
    "        actualizar_lista()\n",
    "        limpiar_campos()\n",
    "    else:\n",
    "        messagebox.showwarning(\"Advertencia\", \"Por favor, complete todos los campos.\")\n",
    "\n",
    "def eliminar_producto():\n",
    "    seleccion = lista_productos.curselection()\n",
    "    if seleccion:\n",
    "        index = seleccion[0]\n",
    "        productos.pop(index)\n",
    "        actualizar_lista()\n",
    "    else:\n",
    "        messagebox.showwarning(\"Advertencia\", \"Seleccione un producto para eliminar.\")\n",
    "\n",
    "def buscar_producto():\n",
    "    nombre = entrada_nombre.get().lower()\n",
    "    if nombre:\n",
    "        encontrados = [p for p in productos if nombre in p.lower()]\n",
    "        actualizar_lista(encontrados)\n",
    "    else:\n",
    "        messagebox.showwarning(\"Advertencia\", \"Ingrese un nombre para buscar.\")\n",
    "\n",
    "def actualizar_lista(productos_a_mostrar=None):\n",
    "    lista_productos.delete(0, tk.END)\n",
    "    for producto in (productos_a_mostrar or productos):\n",
    "        lista_productos.insert(tk.END, producto)\n",
    "\n",
    "def limpiar_campos():\n",
    "    entrada_nombre.delete(0, tk.END)\n",
    "    entrada_cantidad.delete(0, tk.END)\n",
    "    entrada_precio.delete(0, tk.END)\n",
    "\n",
    "# Botones\n",
    "frame_botones = tk.Frame(ventana)\n",
    "frame_botones.pack(pady=10)\n",
    "\n",
    "boton_agregar = tk.Button(frame_botones, text=\"Agregar\", command=agregar_producto)\n",
    "boton_agregar.grid(row=0, column=6, padx=5)\n",
    "\n",
    "boton_eliminar = tk.Button(frame_botones, text=\"Eliminar\", command=eliminar_producto)\n",
    "boton_eliminar.grid(row=0, column=3, padx=5)\n",
    "\n",
    "boton_buscar = tk.Button(frame_botones, text=\"Buscar\", command=buscar_producto)\n",
    "boton_buscar.grid(row=0, column=2, padx=5)\n",
    "\n",
    "# Lista de productos\n",
    "frame_lista = tk.Frame(ventana)\n",
    "frame_lista.pack(pady=10)\n",
    "\n",
    "lista_productos = tk.Listbox(frame_lista, width=50)\n",
    "lista_productos.pack(side=tk.LEFT)\n",
    "\n",
    "scrollbar = tk.Scrollbar(frame_lista)\n",
    "scrollbar.pack(side=tk.RIGHT, fill=tk.Y)\n",
    "\n",
    "lista_productos.config(yscrollcommand=scrollbar.set)\n",
    "scrollbar.config(command=lista_productos.yview)\n",
    "\n",
    "# Ejecutar la interfaz\n",
    "ventana.mainloop()\n"
   ]
